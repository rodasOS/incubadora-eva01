---
import Layout from '../layouts/Layout.astro';
const response = await fetch('http://localhost:4321/api/ejemplo.json');
const data = await response.json();

interface User {
	author: string;
	body: string;
}
---

<Layout title='Dashboard'>
	<ul>
		{
			data.map((d: User) => {
				return <li>{d.author}</li>;
			})
		}
	</ul>
</Layout>

<script></script>
